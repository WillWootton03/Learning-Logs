{
    "builds" : [
        {
            "src" : "learning_logs/wsgi.pi",
            "use" : "@vercel/python"
        }
    ],
    "routes" : [
        {
            "src" : "/static/(.*)",
            "dest" : "/staticfiles/$1"
        },
        {
            "src" : "/(.*)",
            "dest" : "learning_logs/wsgi.py"
        }
    ]
}