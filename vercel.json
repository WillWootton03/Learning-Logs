{
    "builds" : [
        {
            "src" : "learning_logs/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdaSize" : "15mb", "runtime" : "python3.12"}
        },
        {
            "src" : "build.sh",
            "use" : "@vercel/static-build",
            "config" : {
                "distDir" : "staticfiles"
            }
        }
    ],
    "routes" : [
        {
            "src" : "/favicon.ico", 
            "dest" : "/staticfiles/favicon.ico"
        },
        {
            "src" : "/favicon.png",
            "dest" : "/staticfiles/favicon.ico"
        },
        {
            "src" : "/(.*)",
            "dest" : "learning_logs/wsgi.py"
        }
    ],
    "outputDirectory" : "staticfiles"
}