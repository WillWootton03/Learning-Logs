{
    "builds" : [
        {
            "src" : "build.sh",
            "use" : "@vercel/static-build"
        },
        {
            "src" : "learning_logs/wsgi.py",
            "use" : "@vercel/python"
        }
    ],
    "outputDirectory" : "theme/static/dist",
    "routes" : [
        {
            "src" : "/static/(.*)",
            "dest" : "/staticfiles/$1"
        },
        {
            "src" : "/(.*)",
            "dest" : "learning_logs/wsgi.py"
        }
    ]
}