{
    "builds" : [
        {
            "src" : "learning_logs/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdaSize" : "15mb", "runtime" : "python3.12"}
        }
    ],

    "routes" : [
        {
            "src" : "/static/(.*)",
            "dest" : "/staticfiles/$1"
        },
        {
            "src" : "/favicon.ico", 
            "dest" : "/staticfiles/favicon.ico"
        },
        {
            "src" : "/(.*)",
            "dest" : "learning_logs/wsgi.py"
        }
    ]
}